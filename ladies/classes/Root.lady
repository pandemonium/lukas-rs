use Stdlib.

signature Monoid ::= ∀α.
  { mempty  :: α
    mappend :: α -> α -> α
  }

witness :: Monoid Int :=
  { mempty  : 0
    mappend : λp q. p + q
  }

mconcat :: ∀α. Monoid α |- List α -> α :=
  λxs.
    List.fold_right mappend mempty xs



start := λ_.
  let xs = List.tabulate 10 id in
  print_endline "Hi, mom: `mconcat xs`"
